node {
    stage('SCM') {
    	echo 'Gathering code from version control'
	    git branch: '${branch}', url: 'https://github.com/astw/JenkinsGroovy.git'
    }
    stage('Build') {
        echo 'Building....'
	    dotnet --version
        dotnet build ConsoleApp1
	    echo 'Building new feature'
    }
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
}